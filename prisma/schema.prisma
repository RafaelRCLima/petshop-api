generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Service {
  id           String   @id @default(uuid())
  name         String
  description  String
  animalType   String
  size         String
  furSize      String
  race         String
  furIsTangled Boolean
  duration     Int
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("services")
}
